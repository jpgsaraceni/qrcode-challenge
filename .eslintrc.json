{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-undef":"off",
      "no-unused-vars":"off",
      "no-console": "off",
      "prefer-promise-reject-errors": "off",
      "linebreak-style": "off"
    }
}
